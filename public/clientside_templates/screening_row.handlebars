                <tr class="data-row">
                      <td style=" padding-top: 10px;">
                          <form id = "screening_"{{proposal_id}} >
                          <span>
                           <label>
                              <input type="checkbox" class="filled-in checkbox-blue-grey"  />
                          <span></span>
                          </label>
                           </span>
                          </form>
                      </td>
                  <td> <a style="color: #8B83BA;" href="#!"><i class="material-icons">chevron_right</i></a></td>
                  
                  <td class="proposal">   
                      <div class="test" style="overflow-wrap: break-word; word-wrap: break-word;">
                          <a href="!#" style="padding-top: 0px !important; margin-top: -10px; text-align: start; color: #4A4AFF; font-weight: bold; font-size: 12px;">
                          {{ protocol_code }}
                          </a> 
                          <br>
                          {{ title}}
                          <br>

                          <div class="proponent-proposal hide-on-large-medium" >
                            <label style="font-size: 10px; color:#6E6893; font-weight: bold;">Proponents </label>
                            {{#each proponents}}
                            <div><b>{{name}}</b> ({{#if college}} {{college}} {{/if}} {{#if center}}, {{center}} {{/if}} )</div>
                            {{/each}}
                        </div>
                      </div>
                  </td>
                  <td class="details hide-on-medium-small" >
                    <div class="test">
                      {{category}}<br>
                       AY {{AY}}, T{{term}}<br>
                      {{phreb_category}} <br>
                      <div class = "proponents">
                      {{#each proponents}}
                      <div><b>{{name}}</b> ({{#if college}} {{college}} {{/if}} {{#if center}}, {{center}} {{/if}} )</div>
                      {{/each}}
                      </div>
                    </div>
                  </td>
                  <td class = "status" style="width: auto; min-width: 125px; max-width: 120px;">
                    
                 
                    <div class="status-type {{#get_status status}}{{/get_status}}"> 
                      <span class = "circle  {{#get_status status}}{{/get_status}}"></span>
                      {{#get_status status}}
                      {{/get_status}}
                    </div>


    
                    <div class="due_date">due {{due_date}}</div>
                    <div class="assigned_ra"><label style="font-size: 10px; color:#6E6893; font-weight: bold;">Assigned to</label> <br>
                      <a href = "!#" style="font-weight: bold">{{ra}}</a>
                    </div>
                  </td>
                  <td class ="hide-on-medium-small recent_activity" style="width: auto; min-width: 212px; max-width: 300px;">
                    {{!-- <div class = "activity">{{activity}}</div> --}}
                    <p class ="comments">{{comments}}</p>
                  <td>
                  <td class="hide-on-medium-small action-row" style="font-size: 12px; align-items: center;">
                    <a href="!#" class="btn-flat" style="color: #6E6893; text-align: center; font-weight: bold; font-size: 12px; padding-top: 30px;">
                    View More<i class="material-icons"></i></a>
                  </td>
                  <td style="width: 5px;"> <a style="color: #8B83BA;"href="#!"><i class="material-icons">more_vert</i></a></td>
                  </form>
            </tr>